{
	"plugins": [
		{
			"name": "Auto image",
			"className": "AutoImage",
			"description": "Allows for inserting images simply by pasting a URL to the image into the content. Turns the pasted URL into an image on the go.",
			"docs": "features/image.html#inserting-images-via-pasting-url-into-editor",
			"path": "src/autoimage.js",
			"requires": [
				[
					"ImageBlock",
					"ImageInline"
				]
			]
		},
		{
			"name": "Block image",
			"className": "ImageBlock",
			"description": "Enables support for inserting images between the blocks of text, like paragraphs or block quotes. This functionality is also provided by the main Image plugin. Note: The image feature is introduced in a granular form, implemented by separate plugins responsible for such aspects as image caption, toolbar, resize, upload or styles.",
			"path": "src/imageblock.js",
			"htmlOutput": [
				{
					"elements": "figure",
					"classes": "image"
				},
				{
					"elements": "img",
					"attributes": [
						"alt",
						"sizes",
						"src",
						"srcset",
						"width"
					]
				}
			]
		},
		{
			"name": "Inline image",
			"className": "ImageInline",
			"description": "Enables support for inserting images inside the blocks of text, like paragraphs, header or block quotes. This functionality is also provided by the main Image plugin. Note: The image feature is introduced in a granular form, implemented by separate plugins responsible for such aspects as image toolbar, resize, upload or styles.",
			"path": "src/imageinline.js",
			"htmlOutput": [
				{
					"elements": "img",
					"attributes": [
						"alt",
						"sizes",
						"src",
						"srcset",
						"width"
					]
				}
			]
		},
		{
			"name": "Image",
			"className": "Image",
			"description": "Enables support for image handling. Note: The image feature is introduced in a granular form, implemented by separate plugins responsible for such aspects as image caption, toolbar, resize, upload or styles.",
			"docs": "features/image.html",
			"path": "src/image.js",
			"uiComponents": [
				{
					"type": "Button",
					"name": "imageTextAlternative",
					"iconPath": "",
					"toolbars": [
						"image.toolbar"
					]
				}
			],
			"htmlOutput": [
				{
					"elements": "figure",
					"classes": "image"
				},
				{
					"elements": "img",
					"attributes": [
						"alt",
						"sizes",
						"src",
						"srcset",
						"width"
					]
				}
			]
		},
		{
			"name": "Image caption",
			"className": "ImageCaption",
			"description": "Allows for adding captions to block images to give additional context.",
			"docs": "features/image.html#image-captions",
			"path": "src/imagecaption.js",
			"requires": [
				"ImageBlock"
			],
			"htmlOutput": [
				{
					"elements": "figcaption"
				}
			]
		},
		{
			"name": "Image resize",
			"className": "ImageResize",
			"description": "Allows for resizing images using handles.",
			"docs": "features/image.html#resizing-images",
			"path": "src/imageresize.js",
			"requires": [
				[
					"ImageBlock",
					"ImageInline"
				]
			],
			"htmlOutput": [
				{
					"elements": "figure",
					"classes": "image_resized",
					"styles": [
						"width"
					]
				}
			]
		},
		{
			"name": "Image style",
			"className": "ImageStyle",
			"description": "Allows for using predefined, configurable styles to display the images, for example, as a full width image or side image.",
			"docs": "features/image.html#image-styles",
			"path": "src/imagestyle.js",
			"requires": [
				[
					"ImageBlock",
					"ImageInline"
				]
			],
			"uiComponents": [
				{
					"type": "Button",
					"name": "imageStyle:inline",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-inline.svg"
				},
				{
					"type": "Button",
					"name": "imageStyle:block",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-center.svg"
				},
				{
					"type": "Button",
					"name": "imageStyle:side",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-right.svg"
				},
				{
					"type": "Button",
					"name": "imageStyle:alignLeft",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-inline-left.svg"
				},
				{
					"type": "Button",
					"name": "imageStyle:alignRight",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-inline-right.svg"
				},
				{
					"type": "Button",
					"name": "imageStyle:alignCenter",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-center.svg"
				},
				{
					"type": "Button",
					"name": "imageStyle:alignBlockLeft",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-left.svg"
				},
				{
					"type": "Button",
					"name": "imageStyle:alignBlockRight",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-right.svg"
				},
				{
					"type": "SplitButton",
					"name": "imageStyle:wrapText",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-inline-left.svg"
				},
				{
					"type": "SplitButton",
					"name": "imageStyle:breakText",
					"toolbars": [
						"image.toolbar"
					],
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/object-center.svg"
				}
			],
			"htmlOutput": [
				{
					"elements": "figure",
					"classes": "*",
					"_comment": "The plugin can be configured to set any class names on the `<figure>` element."
				},
				{
					"elements": "figure",
					"classes": [
						"image-style-side"
					]
				},
				{
					"elements": "figure",
					"classes": [
						"image-style-align-left"
					]
				},
				{
					"elements": "figure",
					"classes": [
						"image-style-align-right"
					]
				},
				{
					"elements": "figure",
					"classes": [
						"image-style-block-align-center"
					]
				},
				{
					"elements": "figure",
					"classes": [
						"image-style-block-align-left"
					]
				},
				{
					"elements": "figure",
					"classes": [
						"image-style-block-align-right"
					]
				}
			]
		},
		{
			"name": "Image toolbar",
			"className": "ImageToolbar",
			"description": "Introduces a contextual toolbar for images. It appears when an image is selected and can be configured to contain buttons for features such as the text alternative or image styles.",
			"docs": "features/image.html#image-contextual-toolbar",
			"path": "src/imagetoolbar.js",
			"requires": [
				[
					"ImageBlock",
					"ImageInline"
				]
			],
			"registeredToolbars": [
				"image.toolbar"
			]
		},
		{
			"name": "Image upload",
			"className": "ImageUpload",
			"description": "Allows for pasting images from clipboard, dragging and dropping images, selecting them through a file system dialog or from a media management tool. You need to set this plugin up with an official or custom upload adapter.",
			"docs": "features/image.html#image-upload",
			"path": "src/imageupload.js",
			"requires": [
				[
					"ImageBlock",
					"ImageInline"
				]
			],
			"uiComponents": [
				{
					"type": "Button",
					"name": "imageUpload",
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/image.svg"
				}
			]
		},
		{
			"name": "Image insert",
			"className": "ImageInsert",
			"description": "Allows for inserting images via source URL",
			"docs": "features/image.html#inserting-images-via-source-url",
			"path": "src/imageinsert.js",
			"requires": [
				"ImageUpload"
			],
			"uiComponents": [
				{
					"type": "SplitButton",
					"name": "imageInsert",
					"iconPath": "@ckeditor/ckeditor5-core/theme/icons/image.svg"
				}
			]
		}
	]
}
